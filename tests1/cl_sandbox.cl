typedef double3 t_vec3;typedef double t_real;typedef int t_int;typedef struct 			s_mat{	t_vec3			diffuse;	t_real			specular;}						t_mat;typedef struct			s_obj{	t_mat				mat;	t_vec3				pos;	t_vec3				rot;	t_real				r;	t_real				h;}						t_obj;__kernel void k_render(		uint width,		uint height,		uint size,		__global t_obj* scene,		__global t_vec3* test,		__global char* output){	int id = get_global_id(0);	if (id == 0){		int i = 0;		while (i < size)		{			printf("G %f spec: %f ", scene[i].pos.y, scene[i].mat.specular);			i++;		}		printf("\n");		printf("test: 5: %f\n", test[5].x);	}	int x = id % width;	int y = id / height;	output[id] = '#';	// printf("id: %d -> %c\n", id, output[id]);}